{
    "git": {
        "commitMessage": "[VERSION] Release version ${version}"
    },
    "plugins": {
        "/srv/www/cms/release-it-versioning/index.js": {
            "files": {
                "ext_emconf.php": {
                    "type": "regex",
                    "regex": "'version'.=>.'([0-9]+.[0-9]+.*)'"
                }
            },
            "generateChangelog": true
        }
    }
}
